<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/leapgradetracking/db" VERSION="20150119" COMMENT="XMLDB file for Moodle local/leapgradetracking"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="leapgradetracking_log" COMMENT="A log table for Moodle's Leap grade tracking script.">
      <FIELDS>
        <FIELD NAME="id"      TYPE="int"  LENGTH="10"  NOTNULL="true"                 SEQUENCE="true"/>
        <FIELD NAME="type"    TYPE="char" LENGTH="4"   NOTNULL="true" DEFAULT="----"  SEQUENCE="false" COMMENT="Short code for what type of information is contained in this row."/>
        <FIELD NAME="content" TYPE="char" LENGTH="255" NOTNULL="true"                 SEQUENCE="false" COMMENT="Content of the message."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>